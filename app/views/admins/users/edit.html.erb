<h1>アドミン側のユーザー編集ページだよ</h1>
<%= form_for @user do |f| %>

	<%= f.label"ニックネーム" %>
	<%= f.text_field :nickname %>

	<%= f.label "メールアドレス" %>
	<%= f.text_field :email %>

	<%= f.label"苗字"%>
	<%= f.text_field :name_family_name %>

	<%= f.label"名前"%>
	<%= f.text_field :name_name %>

	<%= f.label"苗字フリガナ"%>
	<%= f.text_field :furigana_family_name %>

	<%= f.label"名前フリガナ"%>
	<%= f.text_field :furigana_name_name %>

	<%= f.submit %>
<% end %>
	<%= link_to'このユーザーを削除する',admins_user_path(@user.id), method: :delete, "data-confirm" => "削除しますか？" %>